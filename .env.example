# Environment Variables Example

# Server configuration
PORT=3000
HOST=localhost

# Database configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=myuser
DB_PASSWORD=mypassword
DB_NAME=mydatabase

# JWT configuration
JWT_SECRET=mysecretkey
JWT_EXPIRATION=3600

# API configuration
API_KEY=myapikey
API_SECRET=myapisecret

# Other configuration
LOG_LEVEL=info
